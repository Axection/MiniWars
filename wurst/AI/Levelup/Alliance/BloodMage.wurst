package BloodMage

import Levelup

constant HERO_ID = 'Hblm'

constant ABIL_1 = 'AHfs'
constant ABIL_2 = 'AHbn'
constant ABIL_3 = 'AHdr'
constant ABIL_ULTIMATE = 'AHpx'

init
  new Levelup(HERO_ID)
  ..addTree(ABIL_1, ABIL_2, ABIL_1, ABIL_3, ABIL_2, ABIL_ULTIMATE, ABIL_1, ABIL_2, ABIL_3, ABIL_3)
  ..create()
