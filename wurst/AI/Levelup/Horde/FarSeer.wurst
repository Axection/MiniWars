package FarSeer

import Levelup

constant HERO_ID = 'Ofar'

constant ABIL_1 = 'AOcl'
constant ABIL_2 = 'AOfs'
constant ABIL_3 = 'AOsf'
constant ABIL_ULTIMATE = 'AOeq'

init
  new Levelup(HERO_ID)
  ..addTree(ABIL_1, ABIL_3, ABIL_1, ABIL_3, ABIL_1, ABIL_ULTIMATE, ABIL_3, ABIL_2, ABIL_2, ABIL_2)
  ..create()
